{
  "name": "solution-center-feedback",
  "description": "Feedback tool for rating services provided by Brand Solutions",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/solutioncenter.feedback.js",
  "homepage": "https://github.com/zalando-incubator/solution-center-feedback",
  "scripts": {
    "init": "npm run clean && npm run lint",
    "prebuild": "npm run init",
    "build": "webpack --config webpack/webpack.build.js --NODE_ENV=production",
    "clean": "rimraf \"src/**/*.{js,js.map,css}\" & rimraf \"{coverage,dist}\"",
    "lint": "tslint \"src/**/*.ts\" -t verbose",
    "postinstall": "typings install",
    "prestart": "npm run init",
    "start": "webpack --config webpack/webpack.dev.js --watch --NODE_ENV=dev",
    "pretest": "npm run lint",
    "test": "karma start --NODE_ENV=test"
  },
  "authors": [
    "Damián García <damian.garcia@zalando.de>",
    "Greg Deane <greg.james.deane@zalando.de>"
  ],
  "maintainers": [
    "Damián García <damian.garcia@zalando.de>",
    "Greg Deane <greg.james.deane@zalando.de>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/solution-center-feedback.git"
  },
  "bugs": {
    "url": "https://github.com/zalando-incubator/solution-center-feedback/issues"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-cookies": "^1.5.8",
    "dress-code": "^2.0.0-beta",
    "jquery": "^3.1.0",
    "lodash": "^4.15.0",
    "solution-center-communicator": "^2.2.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.13.2",
    "babel-istanbul": "^0.11.0",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^2.4.1",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor": "^0.2.1",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.8.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.14.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  }
}
